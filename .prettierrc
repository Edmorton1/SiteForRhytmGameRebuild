{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "singleQuote": true,
  "bracketSpacing": false,
  "trailingComma": "none",
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "editorconfig": false,
  "embeddedLanguageFormatting": "auto"
}
